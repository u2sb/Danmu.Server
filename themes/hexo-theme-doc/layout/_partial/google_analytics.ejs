<% if (config.theme_config.google_analytics){ %>
<script async src="https://www.googletagmanager.com/gtag/js?id=<%= config.theme_config.google_analytics %>"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());
  gtag('config', '<%= config.theme_config.google_analytics %>');
</script>
<% } %>
