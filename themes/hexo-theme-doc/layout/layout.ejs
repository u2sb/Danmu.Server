<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">

    <title><%- config.title %> | <%- page.title %> </title>
    <meta name="description" content="<%- page.description %>">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <%- project_partial('head_start') %>

    <!-- fonts -->
    <link href="//fonts.loli.net/css?family=Source+Sans+Pro:400,700" rel="stylesheet">
    <link href="//fonts.loli.net/css?family=Ubuntu:300,400,500,600,700" rel="stylesheet">

    <!-- stylesheets -->
    <link rel="stylesheet" href="<%- url_for('style/doc.css') %>">

    <!-- favicon -->
    <link rel="icon" href="<%- url_for(config.theme_config.favicon) %>">

    <%- project_partial('head_end') %>
    <% if (config.offline){ %>
      <%- partial('_partial/service-worker') %>
    <% } %>
  </head>
  <body>

   <script>window.__INITIAL_STATE__ = <%- JSON.stringify(initial_state) %></script>

    <%- partial('_partial/load_script')%>
    <%- partial('_partial/navigation') -%>
    <%- body %>
    <%- partial('_partial/google_analytics') %>

    <%- project_partial('footer_start') %>

    <!-- js vendors -->
    <script src="https://cdn.jsdelivr.net/npm/jquery/dist/jquery.min.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/lunr@2.1.0/lunr.min.js"></script>

    <!-- js source  -->
    <script src="<%- url_for('script/doc.js') %>"></script>
    <%- partial('_partial/pangu')%>
    
    <%- project_partial('footer_end') %>

  </body>
</html>
